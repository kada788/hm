#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/require@2.4.20/node_modules/require/bin/node_modules:/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/require@2.4.20/node_modules/require/node_modules:/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/require@2.4.20/node_modules:/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/require@2.4.20/node_modules/require/bin/node_modules:/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/require@2.4.20/node_modules/require/node_modules:/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/require@2.4.20/node_modules:/mnt/c/Users/Wzy/Desktop/workspace/hm/ai/cursor/crawl/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/require-command.js" "$@"
else
  exec node  "$basedir/../../bin/require-command.js" "$@"
fi
